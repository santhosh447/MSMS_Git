@model IEnumerable<MSMS.Areas.Admins.Models.Store_Registration>

@{
    ViewBag.Title = "StoreList";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutDesign.cshtml";
}

<h2>StoreList</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StoreName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.License_Number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.License_Image_Copy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tin_Number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Discount_Percentage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Owner_Registration.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StoreName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.License_Number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.License_Image_Copy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tin_Number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Discount_Percentage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Owner_Registration.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.StoreEmail }) |
            @Html.ActionLink("Details", "Details", new { id=item.StoreEmail }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StoreEmail })
        </td>
    </tr>
}

</table>
